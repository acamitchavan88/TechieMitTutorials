package exceptionDemo;

public class ClassNotFoundExceptionDemo {

	public static void main(String[] args)  {
		String classToBeloaded="exceptionDemo.Student";
		try
		{
			Class.forName(classToBeloaded);
			ClassLoader.getSystemClassLoader().loadClass(classToBeloaded);
		}
		catch(ClassNotFoundException ex)
		{
			ex.printStackTrace();
		}
		
	}

}
